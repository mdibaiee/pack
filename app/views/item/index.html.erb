Items List

<%= link_to 'New', action: :new %>

<br />

<table>
  <tr>
    <th>Name</th>
    <th>URL</th>
    <th>Path</th>
  </tr>
<% @items.each do |item| %>
  <tr>
    <td><%= link_to item.name, item_path(item) %></td>
    <td><a href='<%= item.url %>'><%= item.url %></a></td>
    <td><%= item.path %></td>
  </tr>
<% end %>
</table>
