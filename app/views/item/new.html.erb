<%= form_for :item, url: item_index_path do |f| %>
  <%= f.label :file %>
  <%= f.file_field :file %>

  <br />
  <%= f.submit %>
<% end %>

<% if @item and @item.errors.any?
    @item.errors.full_messages.each do |msg| %>

    <p> <%= msg %> </p>
<% end
end %>

<%= link_to 'Back', item_index_path %>
